<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.MilitechController"
            prefHeight="400.0" prefWidth="600.0">
	<VBox>
		<TableView fx:id="militechTable">
			<columns>
				<TableColumn fx:id="id" text="Tech id"/>
				<TableColumn fx:id="techType" text="Technique type"/>
				<TableColumn fx:id="description" text="Description"/>
				<TableColumn fx:id="amount" text="Amount"/>
			</columns>
		</TableView>
		<Label text="Tech type"/>
		<TextField fx:id="techTypeField"/>
		<Label text="Description"/>
		<TextField fx:id="descField"/>
		<Label text="Amount"/>
		<TextField fx:id="amountField"/>
		<Button text="Add technique" onMouseClicked="#addTech"/>
		<Button text="Edit technique data" onMouseClicked="#editTech"/>
		<Button text="Delete technique data" onMouseClicked="#delTech"/>
	</VBox>
</AnchorPane>
