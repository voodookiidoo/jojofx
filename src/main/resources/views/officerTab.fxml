<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.OfficerTabController"
            prefHeight="400.0" prefWidth="600.0">
	<VBox>

		<TableView fx:id="officerTable">
			<columns>
				<TableColumn fx:id="id" text="id"/>
				<TableColumn fx:id="fullname" text="fullname"/>
				<TableColumn fx:id="militaryRank"/>
				<TableColumn fx:id="education"/>
				<TableColumn fx:id="rankDate"/>
				<TableColumn fx:id="major"/>
				<TableColumn fx:id="commType"/>
			</columns>
		</TableView>
	</VBox>
</AnchorPane>
